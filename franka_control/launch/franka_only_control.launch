<?xml version="1.0" ?>
<launch>
  <arg name="robot_ip_1" default="172.16.0.101" />
  <arg name="robot_ip_2" default="172.16.0.102" />
  <arg name="load_gripper" default="false" />

  <param name="robot_description" command="$(find xacro)/xacro --inorder '$(find franka_description)/robots/panda_arm_hand.urdf.xacro'" if="$(arg load_gripper)" />
  <param name="robot_description" command="$(find xacro)/xacro --inorder '$(find franka_description)/robots/panda_arm.urdf.xacro'" unless="$(arg load_gripper)" />

  <node name="franka_control_1" pkg="franka_control" type="franka_control_node_initial_position" output="screen" required="true">
    <rosparam command="load" file="$(find franka_control)/config/franka_control_node_softbots.yaml" />
    <param name="robot_ip" value="$(arg robot_ip_1)" />
  </node>

  <node name="franka_control_2" pkg="franka_control" type="franka_control_node_initial_position" output="screen" required="true">
    <rosparam command="load" file="$(find franka_control)/config/franka_control_node_softbots.yaml" />
    <param name="robot_ip" value="$(arg robot_ip_2)" />
  </node>

</launch>
